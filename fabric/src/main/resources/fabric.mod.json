{
  "schemaVersion": 1,
  "id": "guithium",
  "version": "${version}",
  "name": "Guithium",
  "description": "Allows servers to add GUIs to your client",
  "authors": [
    "BillyGalbreath"
  ],
  "contact": {
    "homepage": "https://github.com/BillyGalbreath/Guithium",
    "sources": "https://github.com/BillyGalbreath/Guithium",
    "issues": "https://github.com/BillyGalbreath/Guithium/issues"
  },
  "license": "MIT",
  "icon": "assets/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "net.pl3x.guithium.fabric.Guithium"
    ],
    "modmenu": [
      "net.pl3x.guithium.fabric.configuration.ModMenu"
    ]
  },
  "mixins": [
    "guithium.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": "1.19.2"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/nhGzEkwXQX"
      }
